  "version": "0.1.1",
  "description": "WebTransport polyfill using WebSockets",
  "type": "module",
  "license": "(MIT OR Apache-2.0)",
  "repository": "github:kixelated/web-transport",
  "exports": {
    ".": "./src/index.ts"
